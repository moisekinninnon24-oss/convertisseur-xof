{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/sitemap.xml",
      "dest": "/public/sitemap.xml"
    },
    {
      "src": "/robots.txt",
      "dest": "/public/robots.txt"
    },
    {
      "src": "/google5b9d6a33a63bfe61.html",
      "dest": "/public/google5b9d6a33a63bfe61.html"
    },
    {
      "src": "/euro-fcfa",
      "dest": "/public/pages/euro-fcfa.html"
    },
    {
      "src": "/dollar-fcfa",
      "dest": "/public/pages/dollar-fcfa.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
