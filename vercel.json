{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/robots.txt",
      "dest": "/public/robots.txt"
    },
    {
      "src": "/google5b9d6a33a63bfe61.html",
      "dest": "/public/google5b9d6a33a63bfe61.html"
    },
    {
      "src": "/sitemap(-v2|-[0-9]+)?\\.xml",
      "dest": "server.js"
    },
    {
      "src": "/blog",
      "dest": "/public/blog.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
